<h1>Listing owners</h1>

<table>
  <tr>
    <th>Full name</th>
    <th>Email</th>
    <th>Address one</th>
    <th>Address two</th>
    <th>Zip</th>
    <th>City</th>
    <th>State</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @owners.each do |owner| %>
  <tr>
    <td><%= owner.full_name %></td>
    <td><%= owner.email %></td>
    <td><%= owner.address_one %></td>
    <td><%= owner.address_two %></td>
    <td><%= owner.zip %></td>
    <td><%= owner.city %></td>
    <td><%= owner.state %></td>
    <td><%= link_to 'Show', owner %></td>
    <td><%= link_to 'Edit', edit_owner_path(owner) %></td>
    <td><%= link_to 'Destroy', owner, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Owner', new_owner_path %>
